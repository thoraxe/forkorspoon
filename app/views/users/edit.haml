#user-functions
  - @user.password = @user.password_confirmation = nil
  = error_messages_for :user
  - form_for :user, :url => @user, :html => {:method => :put} do |f|
    =render :partial => "profile_form", :locals => { :f => f }
    %p== #{submit_tag 'Update'} #{link_to 'Cancel', user_path(current_user)}
