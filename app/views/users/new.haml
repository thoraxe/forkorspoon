#user-functions
  %h3 Sign up as a new user
  - @user.password = @user.password_confirmation = nil
  = error_messages_for :user
  - form_for :user, :url => @user, :html => {:method => :post} do |f|
    =render :partial => "profile_form", :locals => { :f => f }
    %p== #{submit_tag 'Sign Up!'} #{link_to 'Cancel', '/'}
